{
  "framework": null,
  "outputDirectory": "frontend",
  "installCommand": "",
  "buildCommand": "",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD -- frontend/ vercel.json",
  "rewrites": [
    {"source": "/", "destination": "/index.html"},
    {"source": "/favicon.ico", "destination": "/icons/icon-192x192.png"}
  ],
  "headers": [
    {
      "source": "/js/config.js",
      "headers": [
        {"key": "Cache-Control", "value": "no-cache, no-store, must-revalidate"}
      ]
    }
  ]
}
